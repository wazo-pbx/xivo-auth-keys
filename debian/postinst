#!/bin/sh

set -e

case "$1" in
    configure)
        xivo-update-keys xivo-agentd-cli
        xivo-update-keys xivo-agid
        xivo-update-keys xivo-ctid
        xivo-update-keys xivo-ctid-ng
        xivo-update-keys xivo-dird-phoned
    ;;

    abort-upgrade)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
